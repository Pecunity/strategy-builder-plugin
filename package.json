{
  "name": "pecunity-strategy-builder",
  "version": "2.0.1",
  "description": "This is  an ERC-6900 compatible module called StrategyBuilder, built in Foundry. It has one function that can be called through a user operation, called `increment`. In `/src` you will find this plugin, documented so you can understand how it works in detail.",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "/contracts/**/*.sol",
    "!/contracts/test/**/*",
    "/assets/*"
  ],
  "scripts": {
    "test": "forge test",
    "compile": "hardhat compile && forge build",
    "coverage": "forge coverage --report lcov && genhtml lcov.info  --output-dir coverage --via-ir",
    "verify": "npx hardhat ignition verify chain-$CHAIN_ID --include-unrelated-contracts",
    "deploy:core": "hardhat compile && hardhat run scripts/deploy-core.ts --network $NETWORK",
    "deploy:conditions": "hardhat compile &&  hardhat run scripts/deploy-conditions.ts --network $NETWORK"
  },
  "keywords": [],
  "author": "3Blocks",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-foundry": "^1.1.3",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomicfoundation/hardhat-verify": "2.1.1",
    "dotenv": "^16.4.7",
    "hardhat": "^2.26.0",
    "hardhat-deploy": "^0.14.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@erc6900/reference-implementation": "github:erc6900/reference-implementation#v0.8.0",
    "@pythnetwork/pyth-sdk-solidity": "^4.0.0",
    "account-abstraction": "github:eth-infinitism/account-abstraction#v0.7.0"
  }
}
